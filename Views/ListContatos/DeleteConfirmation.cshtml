@model ContatoModel
@{
    ViewData["Title"] = "Apagar contato";
}

<div class="text-center">
    <h3 class="display-4">Apagar o contato (@Model.Name)</h3>
    <a class="btn btn-secondary" asp-area="" asp-controller="Contato" asp-action="Index">Cancelar exclusão</a>
    <a class="btn btn-danger" asp-area="" asp-route-id="@Model.Id" asp-controller="Contato" asp-action="Delete">Confirmar exclusão</a>
</div>


